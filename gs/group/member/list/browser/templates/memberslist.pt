<div>
  <div id="adminMembers">
    <h2>Administrators</h2>
    <ul>
      <tal:block repeat="member view/admins">
        <li class="vcard">
          <tal:block define="user member/user"
            replace="structure provider:groupserver.UserImage">
            A photo
          </tal:block>
          <ul>
            <li><a href="#" class="fn url" tal:content="member/name"
                  tal:attributes="href member/url">A member</a></li>
            <li class="role"
              tal:content="python:(member in view.managers) and 'Manager' or 'Participation Coach'">Batman</li>
          </ul>
        </li>
      </tal:block>
    </ul>
  </div><!--adminMembers-->  
  <div id="normalMembers">
    <h2>Normal Members</h2>
    <ul>
      <tal:block repeat="member view/normalMembers">
        <li class="vcard">
          <a href="">
          <tal:block define="user member/user"
            replace="structure provider:groupserver.UserImage">
            A photo
          </tal:block>
          <a href="#" class="fn url" tal:content="member/name"
            tal:attributes="href member/url">A member</a></li>
      </tal:block>
    </ul>
  </div><!--normalMembers-->
  <div id="invitedMembersMembers">
    <!--TODO: * Hide in public groups-->
    <h2>Invited Members</h2>
    <ul>
      <tal:block repeat="member view/invitedMembers">
        <li class="vcard">
          <a href="">
          <tal:block define="user member/user"
            replace="structure provider:groupserver.UserImage">
            A photo
          </tal:block>
          <a href="#" class="fn url" tal:content="member/name"
            tal:attributes="href member/url">A member</a></li>
      </tal:block>
    </ul>
  </div><!--normalMembers-->
</div>

