<html xmlns="http://www.w3.org/1999/xhtml"
  xmlns:tal="http://xml.zope.org/namespaces/tal"
  xmlns:metal="http://xml.zope.org/namespaces/metal"
  metal:use-macro="context/@@groupserver_site_home_layout/page">
  <head>
    <title metal:fill-slot="title">Members:
      <span tal:replace="string:${view/groupInfo/name}: ${view/siteInfo/name}"/>
    </title>
    <!--TODO: Metadata-->
  </head>
  <body>
    <div metal:fill-slot="body" id="gs-group-member-list">
      <h1>Members</h1>
      <p
        tal:replace="structure provider:groupserver.MembersList">
        The member tabs.
      </p>
    </div><!--gs-group-member-list-->
    <tal:block metal:fill-slot="javascript">
      <script src="/++resource++jquery-ui-1.8.9.custom.min.js" 
        type="text/javascript">&#160;</script>
      <script type="text/javascript">
        jQuery.noConflict()
        jQuery(document).ready( function () {
          jQuery("#info-tabs").tabs();
        });
      </script>        

    </tal:block>
  </body>
</html>

